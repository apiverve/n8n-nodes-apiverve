{
	"name": "@apiverve/n8n-nodes-apiverve",
	"version": "0.0.2",
	"description": "n8n community node for APIVerve - Access 300+ utility APIs for validation, conversion, generation, and more",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes",
		"n8n",
		"apiverve",
		"api",
		"validation",
		"conversion",
		"utilities"
	],
	"license": "MIT",
	"homepage": "https://github.com/apiverve/n8n-nodes-apiverve",
	"author": {
		"name": "APIVerve"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/apiverve/n8n-nodes-apiverve.git"
	},
	"engines": {
		"node": ">=18.10"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/APIVerveOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/APIVerve/APIVerve.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^3.0.0",
		"typescript": "^5.5.0"
	}
}